<Window x:Class="Mattenprüfprogramm.Benutzer.BenutzerAnmelden"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Login" Height="196" Width="277" WindowStartupLocation="CenterOwner" Icon="/Mattenprüfprogramm;component/IconB.png">
    <Grid Height="160">
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="88,51,0,0" Name="passwordBox" VerticalAlignment="Top" Width="133" />
        <Button Content="Login" Height="23" HorizontalAlignment="Left" Margin="12,113,0,0" Name="button_Login" VerticalAlignment="Top" Width="75" Click="button_Login_Click" />
        <Label Content="Passwort:" Height="28" HorizontalAlignment="Left" Margin="12,46,0,0" Name="label_Passwort" VerticalAlignment="Top" />
        <TextBox Height="25" HorizontalAlignment="Left" Margin="88,12,0,0" Name="textBox_Name" VerticalAlignment="Top" Width="133" />
        <Label Content="Name: " Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label_Name" VerticalAlignment="Top" />
        <Label Content="" Height="28" HorizontalAlignment="Left" Margin="12,80,0,0" Name="label_message" VerticalAlignment="Top" Width="209" />
    </Grid>
</Window>
